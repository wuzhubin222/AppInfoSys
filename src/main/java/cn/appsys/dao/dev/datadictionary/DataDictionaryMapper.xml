<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.appsys.dao.dev.datadictionary.DataDictionaryMapper">

	<!-- 通过类型编码查询数据字典的相关信息 -->
	<select id="queryByTypeCode" resultType="DataDictionary">
		SELECT * FROM `data_dictionary` WHERE `typeCode` = #{typeCode}
	</select>
	
</mapper>